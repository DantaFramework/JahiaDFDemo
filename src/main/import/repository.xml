<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:jnt="http://www.jahia.org/jahia/nt/1.0"
              xmlns:jcr="http://www.jcp.org/jcr/1.0"
              xmlns:jmix="http://www.jahia.org/jahia/mix/1.0" xmlns:j="http://www.jahia.org/jahia/1.0"
              xmlns:mix="http://www.jcp.org/jcr/mix/1.0" >
    <templateSets jcr:primaryType="jnt:templateSets">
        <JahiaDFDemo jcr:primaryType="jnt:virtualsite" j:installedModules="JahiaDFDemo" j:siteType="templatesSet" >
            <home j:isHomePage="true"
                  jcr:primaryType="jnt:page" j:templateNode="/templateSets/JahiaDFDemo/templates/base/home">
                <j:translation_en jcr:language="en" jcr:mixinTypes="mix:title" jcr:primaryType="jnt:translation" jcr:title="Home"/>
            </home>

            <portlets jcr:primaryType="jnt:portletFolder"/>
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <templates jcr:primaryType="jnt:templatesFolder">
                <files jcr:primaryType="jnt:folder">
                    <themes jcr:primaryType="jnt:folder">
                        <default jcr:primaryType="jnt:folder">

                        </default>
                    </themes>
                </files>
                <contents jcr:primaryType="jnt:contentFolder"/>

                <base jcr:mixinTypes="jmix:renderable jmix:theme" j:view="JahiaDFDemo" jcr:primaryType="jnt:template" j:theme="/templateSets/JahiaDFDemo/templates/files/themes/default">
                    <home jcr:primaryType="jnt:pageTemplate"/>

                    <simple jcr:primaryType="jnt:pageTemplate"/>
                </base>

                <content-template j:applyOn="jnt:content" j:hiddenTemplate="true" j:priority="1" j:view="JahiaDFDemo" jcr:primaryType="jnt:contentTemplate">
                    <pagecontent jcr:primaryType="jnt:contentList">
                        <main-resource-display j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay" />
                    </pagecontent>
                </content-template>

            </templates>
        </JahiaDFDemo>
    </templateSets>
</content>
        